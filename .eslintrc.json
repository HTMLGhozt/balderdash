{
  "extends": [
    "eslint:recommended",
    "plugin:ava/recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "plugins": ["prettier", "ava"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": ["error", { "allow": ["warn", "error", "info"] }]
  }
}
